{
  "compilerOptions": {
    "baseUrl": "./",
    "declaration": true,
    "target": "ES2017",
    "module": "ES2020",
    "strict": true,
    "esModuleInterop": true,
    "rootDir": ".",
    "outDir": "dist",
    "types": ["node"],
    "noEmit": false,
    "paths": {
      "@/*": ["src/*"]
    }
  },
  "exclude": ["node_modules"],
  "extends": "@shunkakinoki/tsconfig",
  "include": ["src"]
}
